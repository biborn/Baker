{
    "name": "baker",
    "version": "0.2.0",
    "description": "baker creates ansible powered virtual machines",
    "engines": {
        "node": ">=7.10.0"
    },
    "main": "baker.js",
    "scripts": {
        "test": "mocha test/bake/*.js",
        "int-test": "mocha test/integration/*.js",
        "build-macos": "pkg --targets latest-macos-x64 --output ./installers/macos/bin/baker ./package.json",
        "build-win": "pkg --targets latest-win-x64 --output ./installers/win/bin/baker.exe ./package.json",
        "build-linux": "pkg --targets latest-linux-x64 --output ./installers/linux/bin/baker ./package.json",
        "package-macos": "npm run build-macos && ./installers/macos/package.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ottomatica/Baker.git"
    },
    "keywords": [
        "baker",
        "ansible",
        "configuration"
    ],
    "author": "ottomatica",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ottomatica/Baker/issues"
    },
    "homepage": "https://github.com/ottomatica/Baker#readme",
    "dependencies": {
        "bluebird": "^3.5.0",
        "camelcase": "^4.1.0",
        "chalk": "^1.1.3",
        "console.table": "^0.9.1",
        "fs-extra": "^3.0.1",
        "hasbin": "^1.2.3",
        "js-yaml": "^3.10.0",
        "mustache": "^2.3.0",
        "node-vagrant": "^1.1.0",
        "ora": "^1.3.0",
        "prompt": "^1.0.0",
        "request": "^2.81.0",
        "require-dir": "^0.3.2",
        "scp2": "^0.5.0",
        "simple-git": "^1.77.0",
        "slash": "^1.0.0",
        "ssh2": "^0.5.5",
        "tmp": "0.0.31",
        "underscore": "^1.8.3",
        "validator": "^8.0.0",
        "yargs": "^8.0.2"
    },
    "pkg": {
        "assets": ["config/**","remotes"],
        "scripts": "lib/**"
    },
    "bin": "cmd.js",
    "devDependencies": {
        "chai": "^4.1.2",
        "grunt": "^1.0.1",
        "innosetup-compiler": "^5.5.62",
        "mocha": "^3.5.3",
        "pkg": "^4.1.0"
    }
}
